{
  "name": "webinarexample",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "localdb:up": "docker compose down && docker compose up",
    "localdb:down": "docker compose down",
    "testdb:up": "docker compose -f docker-compose.test.yaml down -v && docker compose -f docker-compose.test.yaml up",
    "testdb:down": "docker compose -f docker-compose.test.yaml down -v",
    "test": "jest --config=jest.config.ts",
    "test:coverage": "jest --coverage --config=jest.config.ts",
    "test:integration": "jest --runInBand --config=jest.integration.config.ts",
    "prettier": "prettier . --ignore-unknown",
    "prettier:check": "npm run prettier -- --check",
    "prettier:fix": "npm run prettier -- --write --list-different",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "http-status-codes": "^2.3.0",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.10",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.2",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0"
  },
  "lint-staged": {
    "**/*.{ts,js}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  }
}
